## cd
-- сменяет каталог на указанный или по-умолчанию на ~ (home)
# Нормально выполнит
cd ~
cd ~/temp/d.tcibisov/OS/shell
cd ~/temp

# Выдаст ошибку
cd temp/d.tcibisov/OS/shell


## pwd
-- выводит текущий рабочий каталог
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ pwd
/home/perl/temp/d.tcibisov/OS/shell


## echo
-- выводит переданный текст дополняя его \n в конце
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo

perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo 1
1
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo 2
2
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo 3
3
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo 1  2  3
1 2 3
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo 1 2 3\n 1 2 3
1 2 3n 1 2 3
perl@perlworkstation:~/temp/d.tcibisov/OS/shell$ echo 1 2 3\n 1 2 3

## kill


## ps
ps -elF | grep perl | grep exit


